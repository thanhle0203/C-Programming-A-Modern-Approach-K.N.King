#include <stdio.h>
int main(void)
{
    int grade;
    
    printf("Enter mumerical grade: ");
    scanf("%d", &grade);
    
    if (grade < 0 || grade > 100) {
    printf("Illegal grade\n");
    return 0;
    }
    switch (grade  /10 ) {
        case 5:
            printf("Letter grade: F\n");
            break;
        case 6:
            printf("Letter grade: D\n");
            break;
        case 7:
            printf("Letter grade: C\n");
            break;
        case 8:
            printf("Letter grade: B\n");
            break;
        default:
            printf("Letter grade: A\n");
            break;
    }
    
    return 0;;
    
    
}
